<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <include
        layout="@layout/fragment4_top_content"></include>

    <TextView
        android:drawableLeft="@drawable/more_my_bank_card"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:layout_marginTop="20dp"
        android:background="#ffffffff"
        android:text="钱包"
        android:layout_width="match_parent"
        android:layout_height="52dp" />

    <TextView
        android:drawableLeft="@drawable/more_my_favorite"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:layout_marginTop="20dp"
        android:background="#ffffffff"
        android:text="收藏"
        android:layout_width="match_parent"
        android:layout_height="52dp" />
    <View
        android:paddingLeft="20dp"
        android:paddingRight="16dp"
        android:layout_width="wrap_content"
        android:layout_height="0.1dp"/>
    <TextView
        android:drawableLeft="@drawable/more_my_album"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:background="#ffffffff"
        android:text="相册"
        android:layout_width="match_parent"
        android:layout_height="52dp" />
    <View
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_width="wrap_content"
        android:layout_height="0.1dp"/>
    <TextView
        android:drawableLeft="@drawable/more_game"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:background="#ffffffff"
        android:text="卡包"
        android:layout_width="match_parent"
        android:layout_height="52dp" />
    <View
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_width="wrap_content"
        android:layout_height="0.1dp"/>
    <TextView
        android:drawableLeft="@drawable/more_emoji_store"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:background="#ffffffff"
        android:text="表情"
        android:layout_width="match_parent"
        android:layout_height="52dp" />
    <View
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"/>

    <TextView
        android:drawableLeft="@drawable/more_setting"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:drawablePadding="16dp"
        android:background="#ffffffff"
        android:layout_marginTop="20dp"
        android:text="设置"
        android:layout_width="match_parent"
        android:layout_height="52dp" />

</LinearLayout>